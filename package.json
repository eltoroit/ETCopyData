{
	"name": "etcopydatasf",
	"description": "SF Plugin to populate your scratch org and/or developer sandbox with data for multiple related sObjects.",
	"version": "3.0.0",
	"author": "Andres Perez @ElToroIT",
	"bugs": "https://github.com/eltoroit/ETCopyData/issues",
	"type": "module",
	"dependencies": {
		"@oclif/core": "^4",
		"@salesforce/core": "^8",
		"@salesforce/sf-plugins-core": "^12",
		"tslib": "^2"
	},
	"devDependencies": {
		"@oclif/test": "^4",
		"@salesforce/dev-scripts": "^10",
		"@salesforce/ts-sinon": "^1",
		"@types/chai": "^4",
		"@types/mocha": "^10",
		"@typescript-eslint/eslint-plugin": "^7",
		"@typescript-eslint/parser": "^7",
		"chai": "^5",
		"eslint": "^8",
		"eslint-config-prettier": "^9",
		"eslint-plugin-header": "^3",
		"eslint-plugin-import": "^2",
		"eslint-plugin-jsdoc": "^48",
		"eslint-plugin-prettier": "^5",
		"mocha": "^10",
		"nyc": "^17",
		"oclif": "^4",
		"prettier": "^3",
		"sinon": "^18",
		"ts-node": "^10",
		"typescript": "^5"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"files": [
		"/lib",
		"/messages",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/eltoroit/ETCopyData",
	"keywords": [
		"sf-plugin",
		"salesforce",
		"data-migration",
		"etcopydatasf",
		"etcopydata"
	],
	"license": "MIT",
	"oclif": {
		"commands": "./lib/commands",
		"bin": "sf",
		"topics": {
			"ETCopyDataSF": {
				"description": "SF Plugin to populate your orgs with data for multiple related sObjects."
			}
		}
	},
	"repository": "https://github.com/eltoroit/ETCopyData",
	"scripts": {
		"build": "tsc -p .",
		"lint": "eslint src/**/*.ts test/**/*.ts",
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint src/**/*.ts test/**/*.ts",
		"prepack": "rm -rf lib && tsc -b && oclif manifest && oclif readme",
		"test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	}
}
